<!DOCTYPE HTML>
<!--
tinypng
-->
<html>
	<head>
		<title>
			Code Book
		</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
		
		<link rel="stylesheet" href="assets/css/bootstrap.min.css">
		<link rel="stylesheet" href="assets/css/main.css" />
		<script src="https://kit.fontawesome.com/85999f6d09.js" crossorigin="anonymous"></script>

	</head>
	<body class="">
		<!-- Nav tabs -->
		
		<%- include('./partials/nav.ejs') %>
        
		<section class="">
			<div class="container">
				<div class="row">
                
                    <div class="col-md-8 mx-md-auto">
                        <h2>Users</h2>
                        <%listUsers.forEach(e=>{ %>
                        <div class="boxFriend d-flex">
                            <a href="/profile/<%=e.Handle%>"><%=e.Handle%></a>
                            <!-- <div class="d-flex justify-content-end">
                                 <a href="" class="iconRemove"><i class="fas fa-users-slash"></i></a>
                            </div> -->
                        </div>
                        <%});%>
                        
                    </div>
                </div>
			</div>
		</section>


		<section class="">
			<div class="container">
				<div class="row">
                
                    <div class="col-md-8 mx-md-auto">
                        <h2>Articles</h2>
                        <%listArt.forEach(e=>{ %>
                        <div class="boxFriend d-flex">
							<a href="/articles/<%=e.ID%>"><%=e.ArtName%></a>
				
                        </div>
                        <%});%>
                        
                    </div>
                </div>
			</div>
		</section>

		<section class="">
			<div class="container">
				<div class="row">
                
                    <div class="col-md-8 mx-md-auto">
                        <h2>Documentation</h2>
                        <%listDoc.forEach(e=>{ %>
                        <div class="boxFriend d-flex">
							<a href="/Documentation/<%=e.ID%>"><%=e.DocName%></a>
				
                        </div>
                        <%});%>
                        
                    </div>
                </div>
			</div>
		</section>


		<section class="">
			<div class="container">
				<div class="row">
                
                    <div class="col-md-8 mx-md-auto">
                        <h2>Groups</h2>
                        <%listGroups.forEach(e=>{ %>
                        <div class="boxFriend d-flex">
							<a href="/groups/<%=e.ID%>"><%=e.Name_%></a>
				
                        </div>
                        <%});%>
                        
                    </div>
                </div>
			</div>
		</section>

			

		<!-- Scripts -->
		
		<%- include('./partials/Scripts.ejs') %>
		
			<script>
		
				
			</script>
	</body>
</html>
