		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
		
		<link rel="stylesheet" href="assets/css/bootstrap.min.css">
		<link rel="stylesheet" href="assets/css/main.css" />
		<script src="https://kit.fontawesome.com/85999f6d09.js" crossorigin="anonymous"></script>

	</head>
	<body class="">
		<!-- Nav tabs -->
		<nav id='navbar' class="navbar navbar-expand-sm navbar-dark bg-dark navbar-light">
			<div class="container ">
			  <a href="/" class="navbar-brand d-inline-block justify-content-center">
					Code Book
			  </a>
			  <button class="navbar-toggler " data-toggle="collapse" data-target="#navv">
				<span class="navbar-toggler-icon "></span>
			  </button>
			  <div id="navv" class="collapse navbar-collapse justify-content-between">
				<ul class="navbar-nav ">
					<li class="nav-item hvr-underline-from-left">
						<a href="/" class="nav-link">Home</a>
				   </li>
				  <li class="nav-item hvr-underline-from-left">
					<a href="problemset" class="nav-link">Problem Set</a>
				  </li>
				  <li class="nav-item hvr-underline-from-left">
					<a href="contests" class="nav-link">Contests</a>
				  </li>
				  <li class="nav-item hvr-underline-from-left">
					<a href="articles" class="nav-link">Articles</a>
				  </li>
				  <li class="nav-item hvr-underline-from-left">
					<a href="contact" class="nav-link">Contact</a>
				  </li>
				</ul>
				
				<ul class="navbar-nav ">
					<li class="nav-item">
						<a href="login" class="nav-link">Log In</a>
					</li>
					<span class="slash">/</span>
					<li class="nav-item">
						<a href="signup" class="nav-link">Sign Up</a>
					</li>
					
				</ul>
				
			  </div>
			</div>
		  </nav>
		<section class="">
			<div class="container">
				<div class="w-100 d-flex justify-content-center">
					<div class="w-50 p-5">
						<form action="/signup" method="POST" id="signupform">
							<div class="input-group mb-2">
								<div class="input-group-prepend">
								<span class="input-group-text " id="inputGroupPrepend"><i class="fas fa-user"></i></span>
								</div>
								<input form="signupform" type="text" id="username" name="username" placeholder="Username | Handle" class="form-control form-control-lg mb-md-0 mb-3">
							</div>
							<div class="input-group mb-2">
								<div class="input-group-prepend">
								<span class="input-group-text " id="inputGroupPrepend"><i class="fas fa-user"></i></span>
								</div>
								<input form="signupform" type="text" id="fname" name="fname" placeholder="First Name" class="form-control form-control-lg mb-md-0 mb-3">
							</div>
							<div class="input-group mb-2">
								<div class="input-group-prepend">
								<span class="input-group-text " id="inputGroupPrepend"><i class="fas fa-user"></i></span>
								</div>
								<input form="signupform" type="text" id="lname" name="lname" placeholder="Last Name" class="form-control form-control-lg mb-md-0 mb-3">
							</div>
							<div class="input-group mb-2">
								<div class="input-group-prepend">
								<span class="input-group-text " id="inputGroupPrepend"><i class="fas fa-envelope"></i></span>
								</div>
								<input form="signupform" type="text" id="email" name="email" placeholder="Email" class="form-control form-control-lg mb-md-0 mb-3">
							</div>
							<div class="input-group mb-2">
								<div class="input-group-prepend">
								<span class="input-group-text" id="inputGroupPrepend"><i class="fas fa-lock"></i></span>
								</div>
								<input form="signupform" type="password" id="password" name="password" placeholder="Password" class="form-control form-control-lg">
							</div>
							<div class="form-check form-check-inline">
								<input form="signupform" class="form-check-input" type="radio" name="inlineRadioOptions" id="student" value="student">
								<label class="form-check-label" for="student">Student</label>
							</div>
							<div class="form-check form-check-inline">
								<input form="signupform" class="form-check-input" type="radio" name="inlineRadioOptions" id="developer" value="developer">
								<label class="form-check-label" for="developer">Developer</label>
							</div>
							  
							<button form="signupform" type="submit" class="btn btn-lg btn-block" >sign up</button>
						</form>
						<div>
							<p>alreay have account?</p>
							<a href="login">log in</a>
						</div>
					</div>
				</div>
			</div>
		</section>

			

		<!-- Scripts -->
		
			<script src="assets/js/jquery.min.js"></script>
			<script src="assets/js/browser.min.js"></script>
			<script src="assets/js/breakpoints.min.js"></script>
			<script src="assets/js/util.js"></script>
			<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
			<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.matchHeight/0.7.0/jquery.matchHeight-min.js"></script>

			<script src="assets/js/bootstrap.min.js"></script>
			
			<script src="assets/js/main.js"></script>
		
			<script>
		
				
			</script>
	</body>
