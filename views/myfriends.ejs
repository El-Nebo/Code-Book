                        </h2>
                    </div>

                </div>
            </div>
            <div class="row">
                
                <div class="col-md-8 mx-md-auto">

                    <%listFriends.forEach(e=>{ %>
                    <div class="boxFriend d-flex">
                        <a href="<%=e%>"><%=e%></a>
                        <!-- <div class="d-flex justify-content-end">
                             <a href="" class="iconRemove"><i class="fas fa-users-slash"></i></a>
                        </div> -->
                    </div>
                    <%});%>
                    
                </div>
            </div>

	</section>
		<!-- Scripts -->
		
		<%- include('./partials/Scripts.ejs') %>
	</body>
